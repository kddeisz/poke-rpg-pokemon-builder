{
  "extends": "airbnb",
  "env": {
    "browser": true,
    "node": true,
    "jest": true
  },
  "rules": {
    "arrow-body-style": [2, "as-needed"],
    "arrow-parens": [2, "as-needed"],
    "comma-dangle": 0,
    "function-paren-newline": 0,
    "import/extensions": 0,
    "import/no-unresolved": 0,
    "no-mixed-operators": 0,
    "no-param-reassign": 0,
    "object-curly-newline": 0,
    "quotes": [2, "double"],
    "radix": 0,
    "react/jsx-filename-extension": ["warning", { "extensions": [".js"] }],
    "react/prop-types": 0
  }
}
