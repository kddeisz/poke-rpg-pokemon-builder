{
  "extends": "airbnb",
  "env": {
    "browser": true,
    "node": true,
    "jest": true
  },
  "rules": {
    "arrow-parens": [2, "as-needed"],
    "comma-dangle": 0,
    "no-param-reassign": 0,
    "quotes": [2, "double"],
    "react/jsx-filename-extension": ["warning", { "extensions": [".js"] }],
    "react/prop-types": 0
  }
}
